~~~
system.helpFilesDo('Reference') { :each |
	each.svgCodeBlocks.do { :item |
		let fileName = each.codeBlockImageFileName(item);
		fileName.postLine;
		system.evaluate(item['contents']).writeSvg(fileName)
	}
};

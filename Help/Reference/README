~~~
let verbose = false;
let passCount = 0;
let failCount = 0;
system.referenceHelpFilesDo { :each |
	let scGraphCodeBlocks = each.unspecifiedCodeBlocks;
	scGraphCodeBlocks.isEmpty.ifFalse {
		verbose.ifTrue {
			each.name.postLine
		};
		scGraphCodeBlocks.do { :x |
			let programText = x['contents'];
			let result = system.evaluate(programText);
			result.isOutputSignal.if {
				passCount := passCount + 1
			} {
				failCount := failCount + 1;
				['Failure', each.name, programText].postLine
			}
		}
	}
};
[passCount, failCount].postLine
~~~

